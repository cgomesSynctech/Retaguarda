  object Q_Itens: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    DataSource = Q_MovimentosDS
    SQL.Strings = (
      
        'Select si.*, i.TipoItem, i.Codigo, i.Referencia, si.QtdFaturada ' +
        'as OldQtdFaturada'
      'from SaidasItens si '
      '        LEFT JOIN Itens i ON si.Item = i.Item'
      'Where si.Saida = :SAIDA and si.pdv = :pdv and si.Status <> '#39'H'#39
      'order by si.Ordem')
    Left = 236
    Top = 183
    ParamData = <
      item
        DataType = ftInteger
        Name = 'SAIDA'
        ParamType = ptUnknown
        Size = 4
      end
      item
        DataType = ftUnknown
        Name = 'pdv'
        ParamType = ptUnknown
      end>
  end
