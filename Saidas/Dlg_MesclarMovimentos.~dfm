  object Q_Movimentos: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BeforeOpen = Q_MovimentosBeforeOpen
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select     s.*,'
      '    f.Nome, t.Descricao as DescTipoMov, '
      '    tp.Descricao as DescTipoPadrao,'
      '    v.Nome as NomeVendedor'
      'From Saidas s '
      '    left JOIN Favorecidos f ON s.Favorecido = f.Favorecido'
      '    LEFT JOIN Favorecidos v ON  s.Vendedor = v.Favorecido'
      
        '    INNER JOIN TiposMovimento t ON t.TipoMovimento = s.TipoMovim' +
        'ento'
      '    INNER JOIN TiposPadrao tp ON tp.TipoPadrao = t.TipoPadrao'
      ''
      'Where     s.Situacao = '#39'N'#39'  and KK and  s.TipoPadrao in (ZZ)'
      'and     s.Favorecido in (XX)')
    UniDirectional = True
    Left = 49
    Top = 180
  end
  object P_Movimentos: TDataSetProvider
    DataSet = Q_Movimentos
    Constraints = True
    Left = 49
    Top = 235
  end
