  object Q_Movimentos: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BeforeOpen = Q_MovimentosBeforeOpen
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select '#9'e.*,'
      #9'f.Nome, t.Descricao as DescTipoMov, '
      #9'tp.Descricao as DescTipoPadrao'
      
        'From (((Saidas e INNER JOIN Favorecidos f ON f.Favorecido = e.Fa' +
        'vorecido)'
      
        '         INNER JOIN TiposMovimento t ON t.TipoMovimento = e.Tipo' +
        'Movimento)'
      
        '         INNER JOIN TiposPadrao tp ON tp.TipoPadrao = t.TipoPadr' +
        'ao)'
      ''
      ''
      
        'Where '#9'e.Situacao = '#39'N'#39' and KK and e.TipoPadrao in (ZZ) and e.Fa' +
        'vorecido in (XX)'
      '')
    UniDirectional = True
    Left = 125
    Top = 129
  end
  object P_Movimentos: TDataSetProvider
