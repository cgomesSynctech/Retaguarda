  object Q_Itens: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    DataSource = Q_MovimentosDS
    SQL.Strings = (
      
        'Select ei.*, i.TipoItem, i.Codigo, ei.QtdFaturada as OldQtdReceb' +
        'ida'
      'from SaidasItens ei LEFT JOIN Itens i ON ei.Item = i.Item'
      'Where ei.Saida = :SAIDA and ei.Status in ('#39'P'#39', '#39'L'#39', '#39'F'#39')'
      'order by ei.Ordem')
    Left = 282
    Top = 132
    ParamData = <
      item
        DataType = ftInteger
        Name = 'SAIDA'
        ParamType = ptUnknown
        Size = 4
      end>
  end
