  object Q_Separacao: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    OnCalcFields = Q_SeparacaoCalcFields
    Active = True
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'SELECT l.loteseparacao,'
      '       l.data,'
      '       li.descricao as Localizacao,'
      '       g.descricaogrupo,'
      '       i.Descricao as Item,'
      '       si.fator,'
      '       si.unidade,'
      '       i.referencia, '
      '       iu.QTDEMBALAGEM, '
      '       i.Codigo,'
      '       a.DESCRICAO,'
      '       iu.qtdembalagem,'
      '       Sum(si.QUantidade) as QtdItem'
      'FROM lotesseparacao l'
      
        '   Inner join lotesseparacaosaidas ls on ls.loteseparacao = l.lo' +
        'teseparacao'
      '   Inner join saidas s on s.saida = ls.saida'
      '   Inner join saidasitens si on si.saida = s.saida'
      '   Inner join Itens i on i.item = si.item'
      
        '   Left Join Itensunidades iu on iu.item = i.item and iu.unidade' +
        ' = i.unidade'
      '   Left Join grupos g on i.grupo = g.grupo'
      '   Left Join LOCALIZACOES li ON Li.LOCALIZACAO = I.LOCALIZACAO'
      '   left join ALMOXARIFADOS a on si.ALMOXARIFADO = a.ALMOXARIFADO'
      'where l.loteseparacao = :LOTESEPARACAO'
      'Group By l.loteseparacao,'
      '       l.data,'
      '       li.descricao ,'
      '       g.descricaogrupo,'
      '       i.Descricao,'
      '       si.fator,'
      '       si.unidade,'
      '       i.referencia, '
      '       iu.QTDEMBALAGEM, '
      '       i.Codigo,'
      '       a.DESCRICAO,'
      '       iu.qtdembalagem'
      'Order By li.Descricao desc,  g.descricaogrupo, i.descricao')
    Left = 300
    Top = 297
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'LOTESEPARACAO'
        ParamType = ptUnknown
      end>
    object Q_SeparacaoLOTESEPARACAO: TIntegerField
      FieldName = 'LOTESEPARACAO'
      Required = True
    end
    object Q_SeparacaoDATA: TDateField
