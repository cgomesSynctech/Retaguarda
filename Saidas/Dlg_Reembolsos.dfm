  object Q_Producoes: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BeforeOpen = Q_ItensBeforeOpen
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select pf.*, i.Descricao, fu.Nome'
      'From (ProducoesFunc pf LEFT JOIN Itens i ON i.Item = pf.Item)'
      
        '         LEFT JOIN Favorecidos fu ON fu.Favorecido = pf.Funciona' +
        'rio'
      'Where pf.SAIDAITEM is null  and  pf.NAOCOBRAR = '#39'N'#39' and '
      '            pf.Favorecido = :Favorecido')
    UpdateObject = U_Producoes
    Left = 46
    Top = 111
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Favorecido'
        ParamType = ptUnknown
      end>
  end
  object U_Producoes: TIBUpdateSQL
    RefreshSQL.Strings = (
      'Select '
      'from ProducoesFunc '
      'where'
      '  PRODUCAO = :PRODUCAO')
    ModifySQL.Strings = (
      'update ProducoesFunc'
      'set'
      '  VALORCONTRATO = :VALORCONTRATO,'
      '  NAOCOBRAR = :NAOCOBRAR'
      'where'
      '  PRODUCAO = :OLD_PRODUCAO')
    InsertSQL.Strings = (
      'insert into ProducoesFunc'
      
        '  (PRODUCAO, DATAINICIO, DATAFIM, FAVORECIDO, FUNCIONARIO, CONTR' +
        'ATO, ITEM, '
      
        '   FUNCPAYITEM, QTDHORAS, VALORHORA, VALORCONTRATO, PAYROLL, SAI' +
        'DAITEM, '
      
        '   CHAVECLIENTE, CHAVEFUNC, AGRUPAMENTO, NAOCOBRAR, DESCRICAO, N' +
        'OME)'
      'values'
      
        '  (:PRODUCAO, :DATAINICIO, :DATAFIM, :FAVORECIDO, :FUNCIONARIO, ' +
        ':CONTRATO, '
      
        '   :ITEM, :FUNCPAYITEM, :QTDHORAS, :VALORHORA, :VALORCONTRATO, :' +
        'PAYROLL, '
      
        '   :SAIDAITEM, :CHAVECLIENTE, :CHAVEFUNC, :AGRUPAMENTO, :NAOCOBR' +
        'AR, :DESCRICAO, '
      '   :NOME)')
    DeleteSQL.Strings = (
      'delete from ProducoesFunc'
      'where'
      '  PRODUCAO = :OLD_PRODUCAO')
    Left = 53
    Top = 162
  end
