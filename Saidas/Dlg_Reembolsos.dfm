  object Q_Itens: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BeforeOpen = Q_ItensBeforeOpen
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select '#9'ei.Quantidade, ei.Preco, ei.SubtotalItem, ei.NaoCobrar, ' +
        'ei.Item,'#9#9'ei.SaidaItem, ei.Descricao, ei.Referencia, i.Codigo, e' +
        'i.Unidade, ei.EntradaItem'
      
        'From (EntradasItens ei INNER JOIN Entradas e ON e.Entrada = ei.E' +
        'ntrada)'
      '         LEFT JOIN Itens i ON i.Item = ei.Item'
      'Where '#9'e.TipoPadrao = 100 and ei.SaidaItem is null and '
      
        #9'ei.NaoCobrar = '#39'N'#39'  and e.Situacao = '#39'N'#39'  and ei.ClienteRef = :' +
        'C'
      #9
      #9)
    UpdateObject = U_Itens
    Left = 125
    Top = 116
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'C'
        ParamType = ptUnknown
      end>
  end
