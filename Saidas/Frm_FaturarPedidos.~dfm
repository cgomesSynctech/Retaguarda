  object Q_Titulos: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select tr.id, tr.titulo '
      'from titulosareceber tr '
      'where tr.venda = :saida and tr.pdv = :pdv '
      'order by tr.pdv asc, tr.venda asc, tr.id asc')
    Left = 30
    Top = 280
    ParamData = <
