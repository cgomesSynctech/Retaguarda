  object Q_TiposMovimento: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    Constraints = <
      item
        FromDictionary = False
      end>
    SQL.Strings = (
      'select tm.tipomovimento, tm.descricao, tm.tipopadrao,'
      
        'coalesce(tm.siglainicial, upper(substring(tm.descricao from 1 fo' +
        'r 2))) as siglainicial,'
      'coalesce(tm.baixaestoque, '#39'N'#39') as baixaestoque,'
      'coalesce(tm.baixaestoquefiscal, '#39'N'#39') as baixaestoquefiscal,'
      'coalesce(tm.calccomissao, '#39'N'#39') as calccomissao,'
      'coalesce(tm.gerafinanceiro, '#39'N'#39') as gerafinanceiro,'
      'coalesce(tm.tipoimpressao_op, '#39'N'#39') as tipoimpressao_op,'
      'coalesce(tm.possuiicms, '#39'N'#39') as possuiicms,'
      'coalesce(tm.possuiipi, '#39'N'#39') as possuiipi,'
      'coalesce(tm.possuiiss, '#39'N'#39') as possuiiss,'
      'coalesce(tm.possuientrega, '#39'N'#39') as possuientrega,'
      'coalesce(tm.modelodoc, '#39'01'#39') as modelodoc,'
      'coalesce(tm.cb_notafiscal, '#39'N'#39') as cb_notafiscal,'
      'coalesce(tm.serie,'#39'01'#39') as SerieNota'
      'from tiposmovimento tm'
      'where tm.tipopadrao = 1 and tm.tipomovimento not in (7, 700)'
      'order by tm.tipomovimento asc')
    Left = 607
    Top = 126
