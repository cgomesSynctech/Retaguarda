  object Q_Movimentos: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select'
      '    s.Saida,'
      '    s.numero,'
      '    s.Data,'
      '    f.Favorecido,'
      '    f.Nome as Cliente,'
      '    v.Nome as Vendedor,'
      '    ss.status,'
      '    ss.descricao,       '
      '    s.total,'
      '    s.obs,'
      '    s.PDV,'
      '    s.empresa'
      'From Saidas s'
      '    left JOIN Favorecidos f ON s.Favorecido = f.Favorecido'
      '    LEFT JOIN Favorecidos v ON  s.Vendedor = v.Favorecido'
      '    left join statussaidas ss on s.status = ss.status'
      'Where s.Situacao = '#39'N'#39
      'and s.TipoPadrao in (2)'
      'and s.Status in ( '#39'A'#39','#39'L'#39','#39'P'#39','#39'V'#39')'
      'and s.Data >= :DataInicial'
      'and s.Data <= :DataFinal'
      'order by s.saida asc, s.numero asc')
    UniDirectional = True
    Left = 31
    Top = 114
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DataInicial'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataFinal'
        ParamType = ptUnknown
      end>
  end
