  object Q_Vendas: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select m.descricao|| '#39'/'#39' || a.ano as Descricao, Sum(s.total) as ' +
        'Vendas,'
      'sum(COALESCE((select Sum(e.total) FROM entradasitensmescla m'
      
        '         inner join entradasitens ei on ei.entradaitem = m.entra' +
        'daitem'
      '         inner join entradas e on e.entrada = ei.entrada'
      '      where m.entrada_original = s.saida),0)) as Devolucoes,'
      'Sum(s.total) -'
      'sum(COALESCE((select Sum(e.total) FROM entradasitensmescla m'
      
        '         inner join entradasitens ei on ei.entradaitem = m.entra' +
        'daitem'
      '         inner join entradas e on e.entrada = ei.entrada'
      '      where m.entrada_original = s.saida),0)) As VlrReal'
      ''
      'From Saidas s'
      '        left join Favorecidos f on s.vendedor = f.favorecido'
      '        inner join Meses m on extract(month from s.data) = m.mes'
      '        inner join Anos a on extract(year from s.data) = a.ano'
      'Where s.data >= :DataI'
      ' and s.data <= :DataF'
      ' and s.tipopadrao = 1'
      ' and s.situacao = '#39'N'#39
      'Group by m.descricao,a.ano')
    Left = 295
    Top = 53
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DataI'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataF'
        ParamType = ptUnknown
      end>
    object Q_VendasDESCRICAO: TIBStringField
      FieldName = 'DESCRICAO'
      Size = 32
    end
    object Q_VendasVLRREAL: TFloatField
      FieldName = 'VLRREAL'
    end
    object Q_VendasVENDAS: TFloatField
      FieldName = 'VENDAS'
    end
    object Q_VendasDEVOLUCOES: TFloatField
      FieldName = 'DEVOLUCOES'
    end
  end
  object Q_Detalhe: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select s.vendedor as ID, f.nome as Descricao, Sum(s.total) as Ve' +
        'ndas,'
      ' ((Select sum(e.total) From entradas e Where e.tipopadrao = 106'
      ' and e.situacao = '#39'N'#39
      ' and extract(MONTH from e.data) = extract(MONTH from s.data)'
      
        ' and extract(YEAR from e.data) = extract(YEAR from s.data))) as ' +
        'Devolucoes,'
      
        ' Sum(s.total) - ((Select sum(x.total) From entradas x Where x.ti' +
        'popadrao = 106'
      ' and x.situacao = '#39'N'#39
      ' and extract(MONTH from x.data) = extract(MONTH from s.data)'
      
        ' and extract(YEAR from x.data) = extract(YEAR from s.data))) As ' +
        'VlrReal'
      ' From Saidas s'
      ' left join Favorecidos f on s.vendedor = f.favorecido'
      ' Where s.data >= :DataI and s.data <= :DataF'
      ' and s.situacao = '#39'N'#39
      ' and s.tipopadrao = 1'
      ' Group by s.vendedor, f.nome')
    Left = 310
    Top = 282
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DataI'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataF'
        ParamType = ptUnknown
      end>
    object Q_DetalheID: TIntegerField
      FieldName = 'ID'
    end
    object Q_DetalheDESCRICAO: TIBStringField
      FieldName = 'DESCRICAO'
      Size = 50
    end
    object Q_DetalheVENDAS: TIBBCDField
      FieldName = 'VENDAS'
      Precision = 18
      Size = 3
    end
    object Q_DetalheDEVOLUCOES: TIBBCDField
      FieldName = 'DEVOLUCOES'
      Precision = 18
      Size = 3
    end
    object Q_DetalheVLRREAL: TIBBCDField
      FieldName = 'VLRREAL'
      Precision = 18
      Size = 3
    end
  end
  object Q_VendasDet: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select s.favorecido as ID, f.nome as Descricao, sum(s.total) as ' +
        'Total'
      'From Saidas s'
      '        inner join favorecidos f on s.favorecido = f.favorecido'
      'Where s.data >= :Data and s.data <= :DataF '
      '           and s.situacao = '#39'N'#39' '
      '           and s.tipopadrao = 1'
      'Group by s.favorecido, f.nome')
    Left = 369
    Top = 50
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Data'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataF'
        ParamType = ptUnknown
      end>
    object Q_VendasDetID: TIntegerField
      FieldName = 'ID'
    end
    object Q_VendasDetDESCRICAO: TIBStringField
      DisplayWidth = 15
      FieldName = 'DESCRICAO'
      Size = 50
    end
    object Q_VendasDetTOTAL: TFloatField
      FieldName = 'TOTAL'
    end
  end
  object Q_DetalheNoPeriodo: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select m.descricao || '#39'/'#39' || a.ano as Descricao, sum(si.subtotal' +
        'item) as Total'
      'From Saidas s '
      '         inner join SaidasItens si on s.saida = si.saida'
      
        '         inner join Itens i on si.item = i.item and i.tipoitem I' +
        'N (1,2,3,5,6)'
      
        '         inner join Meses m on extract(month from s.data) = m.me' +
        's'
      '         inner join Anos a on extract(year from s.data) = a.ano'
      'Where s.data >= :DataI and s.data <= :DataF'
      '           and s.situacao = '#39'N'#39' '
      '           and s.tipopadrao = 1'
      '           and si.item = :id'
      'Group by a.ano, m.mes, m.descricao')
    Left = 405
    Top = 283
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DataI'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataF'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'id'
        ParamType = ptUnknown
      end>
    object Q_DetalheNoPeriodoDESCRICAO: TIBStringField
      FieldName = 'DESCRICAO'
      Size = 32
    end
    object Q_DetalheNoPeriodoTOTAL: TIBBCDField
      FieldName = 'TOTAL'
      Precision = 18
      Size = 3
    end
  end
