      end
      object Valores: TMenuItem
        Caption = 'Valor'
        OnClick = ValoresClick
      end
    end
    object Grfico1: TMenuItem
      Caption = 'R'#243'tulo no Gr'#225'fico'
      object VisualizarValores: TMenuItem
        Caption = 'Nenhum'
        OnClick = VisualizarValoresClick
      end
      object PercentualGraf: TMenuItem
        Caption = 'Percentual'
        OnClick = PercentualGrafClick
      end
      object ValoresGraf: TMenuItem
        Caption = 'Valor'
        OnClick = ValoresGrafClick
      end
    end
    object N1: TMenuItem
      Caption = '-'
    end
    object Imprimir: TMenuItem
      Caption = 'Imprimir'
      OnClick = ImprimirClick
    end
  end
end
