  object Q_Invoices: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select  tr.ID,'
      '        f.nome as Favorecido,'
      '        tr.competencia as Data,'
      '        tr.valor,'
      '        tr.parcela,'
      '        tr.vencimento,'
      '        st.descricao as Status,'
      '        tr.status as codStatus,'
      '        tr.notafiscal,'
      '        tr.faltareceber as Saldo,'
      '        tr.venda,'
      '        f.tipofavorecido'
      'From TitulosAReceber tr'
      'Join StatusTitulos st On tr.status=st.status'
      'Join favorecidos f on tr.cliente=f.favorecido'
      'Where tr.cliente=:Favorecido')
