  object Q_SQL: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select m.Descricao || '#39'/'#39' || a.ano as MesExtenso,'
      '(select sum( f_abs(li.valor) )'
      'From Lancamentos l'
      'inner join LancamentosItens li on l.lancamento = li.lancamento'
      'inner join Contas c on li.conta = c.conta'
      
        'inner join TiposContas t on c.tipoconta = t.tipoconta and t.tipo' +
        ' = '#39'D'#39
      
        'where extract(month from l.data) = m.mes and extract(year from l' +
        '.data) = a.ano) as Despesas'
      ','
      '(select sum( f_abs(li.valor) )'
      'From Lancamentos l'
      'inner join LancamentosItens li on l.lancamento = li.lancamento'
      'inner join Contas c on li.conta = c.conta'
      
        'inner join TiposContas t on c.tipoconta = t.tipoconta and t.tipo' +
        ' = '#39'R'#39
      
        'where extract(month from l.data) = m.mes and extract(year from l' +
        '.data) = a.ano) as Receitas'
      'from lancamentos ll'
      'inner join Meses m on extract(month from ll.data) = m.mes'
      'inner join Anos a on extract(year from ll.data) = a.ano'
      'Where ll.data >= :DataI and ll.data <= :DataF'
      'Group by a.ano, m.mes, m.descricao')
    Left = 213
    Top = 52
    ParamData = <
      item
        DataType = ftDate
        Name = 'DataI'
        ParamType = ptUnknown
        Value = '01/01/2000'
      end
      item
        DataType = ftDate
        Name = 'DataF'
        ParamType = ptUnknown
        Value = 37104d
      end>
  end
