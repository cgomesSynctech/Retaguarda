  object Q_Acerto: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'select i.item, i.codigo, i.descricao, i.unidade, i.estoque, i.qt' +
        'deminimo,'
      'max(ei.quantidade) as quantidade'
      'from itens i'
      'inner join entradasitens ei on i.item = ei.item '
      'inner join entradas e on ei.entrada = e.entrada'
      'where i.desativado = '#39'N'#39' and i.estoque <= i.qtdeminimo'
      'and e.favorecido = :fav and e.tipopadrao = 102'
      
        'group by i.item, i.codigo, i.descricao, i.unidade, i.estoque, i.' +
        'qtdeminimo')
    Left = 62
    Top = 112
    ParamData = <
