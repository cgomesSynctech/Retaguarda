  object Q_MesAtual: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select c.conta, c.contapai, c.descricao'
      
        'from contas c inner join tiposcontas t on t.tipoconta = c.tipoco' +
