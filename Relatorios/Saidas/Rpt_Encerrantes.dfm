  object Q_Produto: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select g.descricaogrupo as DescGrupo, s.numero, p.codigo, p.desc' +
        'ricao, p.unidade,p.estoque as estoque,sp.quantidade as QuantTota' +
        'l,sp.preco,sp.quantidade * sp.preco as SubTotal,'
      
        '(sp.quantidade * sp.preco) - (sp.quantidade * p.custocontabil) a' +
        's Lucro,'
      'u.unidade as sigla , s.numero as notafiscal'
      'from Saidas s '
      'LEFT join SaidasItens sp on s.saida = sp.saida'
      'LEFT join Itens p on (sp.item = p.item  and P.ENCERRANTE ='#39'S'#39')'
      'left join unidades u on u.unidade = p.unidade'
      'left join Grupos g on p.grupo = g.grupo'
      
        'where  s.data >= :dDataI and s.data <= :dDataF and s.situacao = ' +
        #39'N'#39
      'and s.tipopadrao in (1,7)'
      '')
    Left = 296
    Top = 5
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'dDataI'
        ParamType = ptUnknown
      end
      item
        DataType = ftDateTime
        Name = 'dDataF'
        ParamType = ptUnknown
      end>
  end
