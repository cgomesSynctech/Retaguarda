  object Q_EncerGrupos: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select g.descricaogrupo as DescGrupo, sum(e.leiturafinal - e.lei' +
        'turainicial) as QuantLitros, sum(e.leiturafinal - e.leiturainici' +
        'al - e.afericao) as VendaLitros, sum(e.afericao) as Afericao, ma' +
        'x(e.preco) as preco, sum((e.leiturafinal - e.leiturainicial - e.' +
        'afericao) * e.preco) as ValorVendas,'
      
        'sum((e.leiturafinal - e.leiturainicial - e.afericao) * e.preco)-' +
        'sum((e.leiturafinal - e.leiturainicial - e.afericao) * p.precoco' +
        'mpra) as Lucro'
      'from Encerrantes e, Itens p, Grupos g'
      'where e.bomba = p.item and p.grupo = g.grupo and '
      'e.Data >= :dDataI and e.Data <= :dDataF')
    Left = 114
    Top = 114
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'dDataI'
        ParamType = ptUnknown
      end
      item
        DataType = ftDateTime
        Name = 'dDataF'
        ParamType = ptUnknown
      end>
  end
