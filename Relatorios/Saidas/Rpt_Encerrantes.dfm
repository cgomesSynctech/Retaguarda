  object Q_Encerrante: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    Active = True
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select e.*,p.descricao,f.loginname as NomeFunc,'
      
        '((e.leiturafinal - e.leiturainicial - e.afericao) * e.preco) - (' +
        '(e.leiturafinal - e.leiturainicial - e.afericao) * p.precocompra' +
        ') as Lucro'
      'from Encerrantes e, Itens p, Usuarios f'
