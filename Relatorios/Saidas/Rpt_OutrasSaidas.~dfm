  inherited Q_Consulta: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    SQL.Strings = (
      
        'select s.saida, s.data, f.nome as nomecliente, f.favorecido, v.n' +
        'ome as nomevendedor, m.nome as nomemedico, gc.descricao as grupo' +
        'cliente, s.totalitens, s.desconto,s.total, s.imposto, s.numero, ' +
        '  p.descricao as descplanopagamento, s.dataentrega, s.valorcomis' +
        'sao, c.descricao as desccaixa, st.descricao as descstatus, s.obs' +
        ',  s.juros, s.frete, f.tipofavorecido, s.vendedor, s.medico, tp.' +
        'descricao as desctabelapreco, f.bairro, f.cpf_cnpj, f.cidade, f.' +
        'bairro, f.cep, f.endereco, f.uf, f.fone1, '
      
        'tm.descricao as desctipomovimento, (select cast(sum(si.customedi' +
        'o * si.quantidade) as numeric(15,2)) from saidasitens si where s' +
        'i.saida = s.saida) as custo'
      'from saidas s'
      '     left join favorecidos f on f.favorecido = s.favorecido'
      '     left join favorecidos v on v.favorecido = s.vendedor'
      '     left join favorecidos m on m.favorecido = s.medico'
      
        '     left join gruposcliente gc on gc.grupocliente = f.grupoclie' +
        'nte'
      
        '     left join planospagamento p on p.planopagamento = s.planopa' +
        'gamento'
      '     left join statussaidas st on st.status = s.status'
      '     left join contas c on c.conta = s.caixa'
      
        '     left join tabelaspreco tp on tp.tabelapreco = s.tabelapadra' +
        'o'
      
        '     left join tiposmovimento tm on tm.tipomovimento = s.tipomov' +
        'imento')
    Left = 340
  end
