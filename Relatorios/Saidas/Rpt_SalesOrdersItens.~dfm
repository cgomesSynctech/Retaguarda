  inherited Q_Consulta: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    SQL.Strings = (
      
        'Select s.saida, s.numero, s.data, f.favorecido as cliente, f.nom' +
        'e,'
      
        'fu.favorecido as vendedor,  fu.nome as NomeVendedor, s.totaliten' +
        's,'
      's.desconto, s.frete, s.outrasdespesas, s.juros,'
      
        's.imposto, s.total, s.baseimposto, ss.descricao as statusdesc, s' +
        'i.preco,'
      
        'si.descricao, si.status, si.subtotalitem, si.saidaitem, si.item,' +
        ' it.descricao as DescItem,'
      
        'si.quantidade, si.qtdfaturada, (select max(numero) from saidas s' +
        '2, saidasitens si2, saidasitensmescla sim'
      'where s2.saida = si2.saida and si2.saidaitem = sim.saidaitem'
      'and sim.saidaitem_original = si.saidaitem) as Invoice,'
      '(select max(s3.saida) from saidas s3, saidasitens si3, '
      'saidasitensmescla sim3'
      'where s3.saida = si3.saida and si3.saidaitem = sim3.saidaitem'
      'and sim3.saidaitem_original = si.saidaitem) as SaidaInvoice,'
      'fab.descricao as Fabricante, z.descricao as zona,'
      
        'V.NOME AS FuncVenda, ((si.precotabela * si.quantidade)* it.comis' +
        'sao/100) as ComissaoPecaTabela,'
      
        '((si.preco * si.quantidade)* it.comissao/100) as ComissaoPecaVen' +
        'da, si.quantidadevolume,'
      
        'case when strlen(extract(month from s.data)) = 2 then extract(mo' +
        'nth from s.data) || '#39'/'#39' || extract(year from s.data) else '#39'0'#39' ||' +
        ' extract(month from s.data) || '#39'/'#39' || extract(year from s.data) ' +
        'end as MesAno'
      ''
      'from saidas s '
      'join favorecidos f on s.favorecido = f.favorecido'
      'inner join SaidasItens si on s.saida = si.saida'
      'left join favorecidos fu on s.vendedor = fu.favorecido'
      'inner join statussaidasitens ss on si.status = ss.status'
      'inner join Itens it on it.item = si.item'
      'left join fabricantes fab on it.fabricante = fab.fabricante'
      'left join zonas z on z.zona = f.zona'
      'LEFT JOIN FAVORECIDOS V ON V.FAVORECIDO = SI.funcproducao'
      'where s.tipopadrao = 2 and s.situacao = '#39'N'#39' and'
      's.data between :idata and :fdata')
    Left = 292
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'idata'
        ParamType = ptUnknown
      end
      item
        DataType = ftDateTime
        Name = 'fdata'
        ParamType = ptUnknown
      end>
  end
