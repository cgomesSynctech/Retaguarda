  object Q_Itens: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'select i.item,  i.codigo, i.referencia, i.descricao, g.descricao' +
        'grupo, i.unidade ,'
      
        '(select cast(coalesce( Sum((y.fator* y.quantidade)/i.fatorundven' +
        'da) ,0) as Numeric(8,3)) from transacoesestoque y'
      '  where y.item = i.item and y.data < :Data) as SaldoAnterior'
      'From Itens I'
      '  Inner join grupos g on g.grupo = i.grupo'
      'order by i.item')
    Left = 189
    Top = 11
  end
  object Q_Transacoes: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BeforeOpen = Q_TransacoesBeforeOpen
    BufferChunks = 1000
    CachedUpdates = False
    DataSource = Q_Master
    SQL.Strings = (
      'Select t.data,  '
      '           t.unidade, '
      '           case when t.numero is not null then t.numero'
      
        '                   when (t.numero is null) and (s.numero is null' +
        ') then e.numero'
      
        '                   when (t.numero is null) and (e.numero is null' +
        ') then s.numero'
      '           end as Numero ,'
      
        '           cast(coalesce((t.fator*t.quantidade)/i.fatorundvenda,' +
        '0) as Numeric(8,3))  As LANCAMENTO,'
      
        '           case when t.quantidade > 0 then cast(coalesce((t.fato' +
        'r*t.quantidade)/i.fatorundvenda,0) as Numeric(8,3)) end  Entrada' +
        ' ,'
      
        '           case when t.quantidade < 0 then cast(coalesce((t.fato' +
        'r*t.quantidade)/i.fatorundvenda,0) as Numeric(8,3)) end  Saida, '
      
        '           (select cast(sum(coalesce((x.fator*x.quantidade)/i.fa' +
        'torundvenda,0)) as Numeric(8,3))  From transacoesestoque x where' +
        ' x.item = t.item and x.data <= t.data) as Saldo'
      'from transacoesestoque t'
      '  inner join itens i on i.item = t.item'
      
        '  left join entradas e on e.entrada = t.idgerador and (t.entrada' +
        'item is not null)'
      
        '  left join saidas s on s.saida = t.idgerador and (t.saidaitem i' +
        's not null)'
      
        'where t.item = :ITEM AND (T.DATA >= :DTINICIO AND T.DATA <= :DTF' +
        'IM)'
      'order by  1'
      '')
    UniDirectional = True
    Left = 311
    Top = 8
  end
