  inherited Q_Consulta: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    SQL.Strings = (
      
        'select pf.producao,cli.nome as NomeCliente, func.nome as NomeFun' +
        'c, func.codigo, i.descricao, pf.favorecido, pf.funcionario, pf.i' +
        'tem, cli.fone1,'
      
        'pf.qtdhoras, pf.valorcontrato, ((pf.qtdhoras * pf.valorcontrato)' +
        ' - pf.despesas) as SubTotalReceber, pf.valorhora, ((pf.qtdhoras ' +
        '* pf.valorhora) - pf.despesas) as SubTotalPagar, pf.qtdhoras_use' +
        'r, pf.MetodoCliente, pf.despesas, pf.datainicio, pf.datafim'
      'from producoesfunc pf '
      'left join favorecidos cli on pf.favorecido = cli.favorecido'
      'left join favorecidos func on pf.funcionario = func.favorecido'
      'left join itens i on pf.item = i.item'
      'where pf.datainicio >= :dDataI and pf.datainicio <= :dDataF'
      ''
      '')
    Top = 111
    ParamData = <
      item
        DataType = ftDateTime
        Name = 'dDataI'
        ParamType = ptUnknown
      end
      item
        DataType = ftDateTime
        Name = 'dDataF'
        ParamType = ptUnknown
      end>
  end
