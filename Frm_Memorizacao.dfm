  object U_Memorizacao: TIBUpdateSQL
    RefreshSQL.Strings = (
      'Select '
      '  MEMORIZACAO,'
      '  DATA,'
      '  ORIGEM,'
      '  IDGERADOR,'
      '  DESCRICAO,'
      '  NUMERO'
      'from MEMORIZACOES '
      'where'
      '  MEMORIZACAO = :MEMORIZACAO')
    ModifySQL.Strings = (
      'update MEMORIZACOES'
      'set'
      '  DATA = :DATA,'
      '  ORIGEM = :ORIGEM,'
      '  IDGERADOR = :IDGERADOR,'
      '  DESCRICAO = :DESCRICAO,'
      '  NUMERO = :NUMERO,'
      '  TIPO = :TIPO,'
      '  FORMNAME = :FORMNAME'
      'where'
      '  MEMORIZACAO = :OLD_MEMORIZACAO')
    InsertSQL.Strings = (
      'insert into MEMORIZACOES'
      
        '  (MEMORIZACAO, DATA, ORIGEM, IDGERADOR, DESCRICAO, NUMERO, TIPO' +
        ', '
      'FORMNAME)'
      'values'
      
        '  (:MEMORIZACAO, :DATA, :ORIGEM, :IDGERADOR, :DESCRICAO, :NUMERO' +
        ', '
      ':TIPO, '
      '   :FORMNAME)')
    DeleteSQL.Strings = (
      'delete from MEMORIZACOES'
      'where'
      '  MEMORIZACAO = :OLD_MEMORIZACAO')
    Left = 60
    Top = 89
  end
