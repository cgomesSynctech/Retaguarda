  object Q_Montagens: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select m.*, i.descricao from montagens m, itens i'
      'where m.item = i.item and m.status = :status and m.tipo = :tipo'
      'and (:bOper = 1 or m.montador = :operador)'
      'order by m.urgente desc, m.data , m.montagem ')
    UpdateObject = U_Montagens
    Left = 467
    Top = 91
    ParamData = <
      item
        DataType = ftInteger
        Name = 'status'
        ParamType = ptUnknown
      end
      item
        DataType = ftString
        Name = 'tipo'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'bOper'
        ParamType = ptUnknown
      end
      item
        DataType = ftInteger
        Name = 'operador'
        ParamType = ptUnknown
      end>
  end
