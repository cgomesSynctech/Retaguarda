  object Q_Geral: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select sum(t.valor-t.valorpago-t.descontos-t.creditoutilizado) a' +
        's Total'
      'From TitulosAReceber t'
      'Where t.Vencimento >= :DataI and t.Vencimento <= :DataF '
      '           and t.Status = 1'
      'UNION'
      'Select Sum(dd.Valor) as Total from DepositosDoc dd'
      'Where dd.Vencimento >= :DataI and dd.Vencimento <= :DataF '
      '            and dd.status = 3')
    Left = 285
    Top = 97
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DataI'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataF'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataI'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataF'
        ParamType = ptUnknown
      end>
