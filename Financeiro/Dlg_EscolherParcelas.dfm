  object Q_Parcelas: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'select t.id, t.titulo, t.vencimento, t.faltareceber, f.nome, f.e' +
        'ndereco, f.cidade, f.uf, f.cep, f.fone1, f.fone2, f.celular'
      'from titulosareceber t'
      'left join favorecidos f on f.favorecido = t.cliente'
      'where t.status > 0 and t.status < 50 and t.romaneio = '#39'N'#39' and'
      't.vencimento >= :dtini  and t.vencimento <= :dtfim'
      'and (t.cliente = :cli or :cli = 0)'
      'order by t.vencimento, f.nome')
    Left = 336
