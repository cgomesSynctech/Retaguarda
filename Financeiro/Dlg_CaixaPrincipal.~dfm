  object Q_PagamentosDet: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    DataSource = Q_PagamentosDS
    SQL.Strings = (
      
        'Select t.favorecido, f.nome, t.data, t.numcheque, t.Valor, t.his' +
        'torico, c.descricao as DescConta'
      
        'From   Transacoes t inner join Favorecidos f on t.favorecido = f' +
        '.favorecido'
      
        '           left join OperacoesFinanceiras o on t.idgerador = o.o' +
        'peracao'
      '           left join Contas c on o.contadestino = c.conta'
      'Where t.Data >= :DataI and t.Data <= :DataF '
      '           and t.Conta = 1 '
      '           and t.tipooperacao = :tipooperacao'
      '           and t.formapagamento = :formapagamento '
      '           and t.TipoTransacao = '#39'D'#39' ')
    Left = 272
    Top = 428
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DataI'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataF'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'tipooperacao'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'formapagamento'
        ParamType = ptUnknown
      end>
