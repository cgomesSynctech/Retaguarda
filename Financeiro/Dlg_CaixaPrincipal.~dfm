  object Q_SaldoPre: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    OnCalcFields = Q_SaldoPreCalcFields
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select fp.Descricao, '
      '            (Select sum(dd.valor) '
      '            from Depositosdoc dd '
      '            inner join Depositos d on dd.deposito = d.deposito'
      
        '            where d.data <= :dataf and dd.ContaAtual = 1 and dd.' +
        'Formapagamento = 3 and dd.Status < 70) as Entradas,'
      '            (Select sum(dd.valor) '
      '            from Depositosdoc dd '
      
        '            inner join Depositos d on (d.empresa = dd.empresa an' +
        'd d.deposito = dd.deposito and d.pdv = dd.pdv)'
      
        '            where dd.dataretirada <= :dataf and d.Conta = 1 and ' +
        'dd.FormaPagamento = 3 and dd.Status < 70) as Saidas'
      'From FormasPagamento fp'
      'Where fp.formapagamento = 3')
    Left = 436
    Top = 292
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'dataf'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dataf'
        ParamType = ptUnknown
      end>
