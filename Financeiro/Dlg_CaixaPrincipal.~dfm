  object Q_Impostos: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select t.Descricao as Tax, sum(s.Imposto+s.Imposto_Incluso) as T' +
        'otal'
      'From Saidas s inner join Taxs t on s.tax = t.tax'
      
        '                       inner join TiposMovimento m on s.tipomovi' +
        'mento = m.tipomovimento'
      
        'Where s.Data >= :DataI and s.Data <= :DataF and s.TipoPadrao = 1' +
        ' '
      '           and s.Situacao = '#39'N'#39
      'Group by t.Descricao')
    Left = 432
    Top = 428
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'DataI'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'DataF'
        ParamType = ptUnknown
      end>
