  object Q_OutrosDocDet: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    DataSource = Q_OutrosDocDS
    SQL.Strings = (
      'select dd.numcheque, d.data, f.nome, dd.Valor'
      
        'from Depositos d inner join DepositosDoc dd on (d.empresa = dd.e' +
        'mpresa and d.deposito = dd.deposito and d.pdv = dd.pdv)'
      '       inner join Favorecidos f on d.favorecido = f.favorecido'
      'where d.data >= :datai and d.data <= :dataf and '
      '          d.Conta = 1 and'
      '          dd.formapagamento = :formapagamento and'
      '          dd.status < 70'
      '          '
      '')
