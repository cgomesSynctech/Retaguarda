  object Q_Disponivel: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select t.formapagamento, fp.descricao, '
      '          sum(t.valor) as Valor'
      'from transacoes t '
      
        '        inner join formaspagamento fp on t.formapagamento = fp.f' +
        'ormapagamento'
      'where  t.data >= :datai and t.data <= :dataf '
      '           and t.conta = 1 '
      '           and t.formapagamento IN (1,2)'
      '           and t.tipotransacao = '#39'C'#39
      'group by fp.descricao, t.formapagamento')
    Left = 16
    Top = 152
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dataf'
        ParamType = ptUnknown
      end>
    object Q_DisponivelFORMAPAGAMENTO: TIntegerField
      FieldName = 'FORMAPAGAMENTO'
    end
    object Q_DisponivelDESCRICAO: TIBStringField
      FieldName = 'DESCRICAO'
      Required = True
      Size = 50
    end
    object Q_DisponivelVALOR: TIBBCDField
      FieldName = 'VALOR'
      Precision = 18
      Size = 2
    end
  end
  object Q_OutrosDoc: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select dd.FormaPagamento, fp.Descricao, sum(dd.Valor) as Valor'
      'from Depositos d '
      
        '        inner join DepositosDoc dd on (d.empresa = dd.empresa an' +
        'd d.deposito = dd.deposito and d.pdv = dd.pdv)'
      
        '        inner join Formaspagamento fp on dd.formapagamento = fp.' +
        'formapagamento'
      'where d.data >= :datai and d.data <= :dataf and d.Conta = 1 and '
      
        '           (not dd.formapagamento IN (1,2)) and dd.formapagament' +
        'o > 0 and '
      '           dd.status < 70'
      'group by fp.Descricao, dd.formapagamento')
    Left = 188
    Top = 152
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dataf'
        ParamType = ptUnknown
      end>
    object Q_OutrosDocFORMAPAGAMENTO: TIntegerField
      FieldName = 'FORMAPAGAMENTO'
    end
    object Q_OutrosDocDESCRICAO: TIBStringField
      FieldName = 'DESCRICAO'
      Required = True
      Size = 50
    end
    object Q_OutrosDocVALOR: TIBBCDField
      FieldName = 'VALOR'
      Precision = 18
      Size = 2
    end
  end
  object Q_SaldoAntDisp: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select t.formapagamento, fp.descricao, '
      '          sum(abs(t.valor)) as Valor'
      'from transacoes t '
      '       inner join formaspagamento fp '
      '      on t.formapagamento = fp.formapagamento'
      
        'where  t.data < :datai and t.conta = 1 and t.formapagamento IN (' +
        '1,2)'
      'group by t.formapagamento, fp.descricao')
    Left = 352
    Top = 152
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end>
    object Q_SaldoAntDispFORMAPAGAMENTO: TIntegerField
      FieldName = 'FORMAPAGAMENTO'
    end
