  object Q_SaldoAntPre: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select dd.formapagamento, fp.descricao, Sum(dd.Valor) as Valor'
      'from Depositos d '
      
        '        inner join DepositosDoc dd on (d.empresa = dd.empresa an' +
        'd d.deposito = dd.deposito and d.pdv = dd.pdv)'
      
        '        inner join FormasPagamento fp on dd.formapagamento = fp.' +
        'formapagamento'
      
        'where (d.data < :datai) and dd.contaatual = 1 and (not dd.formap' +
        'agamento IN (1,2)) and dd.formapagamento > 0 and dd.status < 70'
      'group by dd.formapagamento, fp.descricao')
    Left = 436
    Top = 152
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end>
    object Q_SaldoAntPreFORMAPAGAMENTO: TIntegerField
      FieldName = 'FORMAPAGAMENTO'
    end
