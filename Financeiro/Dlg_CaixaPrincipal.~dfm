    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'select t.contaorigem, c.descricao as NomeConta, t.formapagamento' +
        ', fp.descricao, sum(t.valor) as Valor'
      'from transacoes t '
      
        '        inner join formaspagamento fp on t.formapagamento = fp.f' +
        'ormapagamento'
      '        inner join contas c on t.contaorigem = c.conta'
      'where t.data >= :datai and t.data <= :dataf '
      
        '          and t.conta = 1 and t.contaorigem <> 0 and t.tipotrans' +
        'acao = '#39'C'#39' and t.formapagamento > 0 '
      
        'group by t.contaorigem, c.Descricao, fp.descricao, t.formapagame' +
        'nto')
