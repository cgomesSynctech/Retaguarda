  object Q_RecCxPrincipal: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select t.formapagamento, fp.descricao, sum(t.valor) as Valor'
      
        'from transacoes t inner join formaspagamento fp on t.formapagame' +
        'nto = fp.formapagamento'
      'where t.data >= :datai and t.data <= :dataf '
      
        '          and t.conta = 1 and t.formapagamento > 0 and t.tipotra' +
        'nsacao = '#39'C'#39' and t.situacao is null and t.contaorigem is null '
      'group by fp.descricao, t.formapagamento')
    Left = 16
    Top = 292
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end
