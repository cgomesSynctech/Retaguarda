  object Q_Cheques: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select dd.IDDoc, dd.*, f.Nome, fp.Descricao as DescForma, s.Desc' +
        'ricao as DescStatus, b.Nome as NomeBanco, d.TipoDeposito, '
      
        'c.Descricao as DescConta, d.Data, d.favorecido, f.tipofavorecido' +
        ', v.nome as vendedor'
      'From DepositosDoc dd, '
      '         Depositos d, '
      '         Favorecidos f, '
      '         FormasPagamento fp, '
      '         StatusDoc s, '
      '         Favorecidos b,'
      '         Favorecidos v,'
      '        Contas c'
      
        'Where (dd.Deposito = d.Deposito and dd.empresa = d.empresa and d' +
        'd.pdv = d.pdv)'
      'and d.conta = c.conta '
      'and d.Favorecido = f.Favorecido'
      'and v.favorecido = f.vendedor'
      'and      dd.FormaPagamento = fp.FormaPagamento'
      'and      dd.Status = s.Status'
      'and      fp.Especie = 1'
      ''
      ''
      ' ')
    UpdateObject = U_Cheques
    Left = 26
    Top = 177
  end
