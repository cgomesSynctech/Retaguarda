  object Q_Cartoes: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select dd.*, d.Data, dd.localtitulo as DescLocal, f.Nome, d.Favo' +
        'recido, d.TipoDeposito, fp.encargos'
      'From  DepositosDoc dd, '
      'Favorecidos f, '
      'Depositos d, Formaspagamento fp'
      'Where '
      '           dd.Vencimento <= CURRENT_DATE and '
      '           dd.FormaPagamento = :FormaPag  and '
      '           dd.Status = 1 and '
      '           dd.ForaDaEmpresa = '#39'N'#39' and '
      '           dd.FormaPagamento = fp.formapagamento and   '
      '           fp.Especie = 3 and '
      
        '           (d.empresa = dd.empresa and d.deposito = dd.deposito ' +
        'and d.pdv = dd.pdv) and'
      '           d.Favorecido = f.Favorecido'
      'order by dd.Vencimento ')
    Left = 25
    Top = 196
    ParamData = <
      item
        DataType = ftInteger
        Name = 'FormaPag'
        ParamType = ptUnknown
      end>
  end
