  object Q_Master: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select o.*, oc.Data, c.descricao as DescricaoConta, f.Nome as No' +
        'meCliente'
      'from (OperacoesFinanceirasDet o '
      
        '        INNER JOIN OperacoesFinanceiras oc ON oc.Operacao = o.Op' +
        'eracao)'
      '        LEFT JOIN Contas c ON oc.contadestino = c.conta'
      '        LEFT JOIN Favorecidos f ON o.favorecido = f.favorecido'
      'where o.Operacao = :Operacao'
      ''
      ''
      ' '
      ' '
      ' ')
