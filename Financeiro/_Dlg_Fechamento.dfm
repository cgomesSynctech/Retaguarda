  object Q_Resumo: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select fp.FormaPagamento, fp.Descricao, '
      '           (Select Sum(t.valor) From Transacoes t'
      
        '           Where t.Data < :Data and t.Conta = :Conta and        ' +
        '                                     t.formapagamento = fp.forma' +
        'pagamento) as SaldoAnterior, '
      '           '
      '           (Select Sum(t.valor) From Transacoes t'
      
        '           Where t.Data <= :Data and t.Conta = :Conta and       ' +
        '                                      t.formapagamento = fp.form' +
        'apagamento) as SaldoAtual, '
      '           '
      '           (Select Sum(t.valor) From Transacoes t'
      
        '           Where t.Data = :Data and  t.Conta = :Conta and       ' +
        '                                     t.formapagamento = fp.forma' +
        'pagamento and '
      '           t.TipoTransacao = '#39'C'#39') as Entradas, '
      ''
      '           (Select Sum(t.valor) From Transacoes t'
      
        '           Where t.Data = :Data and  t.Conta = :Conta and       ' +
        '                                     t.formapagamento = fp.forma' +
        'pagamento and '
      
        '           t.TipoTransacao = '#39'D'#39' and t.Situacao is Null) as Said' +
        'as, '
      ''
      '           (Select Sum(t.valor) From Transacoes t'
      '            Where t.Data = :Data and  '
      '            t.Conta = :Conta and '
      '            t.formapagamento = fp.formapagamento '
      '            and t.TipoTransacao = '#39'D'#39' '
      '            and t.Situacao = '#39'R'#39') as Repasse'
      ''
      'From FormasPagamento fp'
      'Where fp.especie in (1,5) and fp.desativado = '#39'N'#39' '
      'Order by fp.FormaPagamento')
    Left = 28
    Top = 216
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Data'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'conta'
