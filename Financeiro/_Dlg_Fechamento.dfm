    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    OnCalcFields = Q_DisponivelCalcFields
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select fp.FormaPagamento, fp.Descricao, '
      '           (Select Sum(t.valor) From Transacoes t'
      
