  object Q_Retiradas: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = True
    SQL.Strings = (
      
        'Select rd.IDDoc, r.Retirada, r.Data, ct.Descricao as DescConta, ' +
        'ct.Agencia, ct.NumConta, rd.NumCheque, rd.Valor,'
      
        'rd.Vencimento, f.Nome, f.TipoFavorecido, sd.Descricao as DescSta' +
        'tus, rd.Obs as Historico, rd.Status, rd.Conta, rd.ChequeNominal,' +
        ' r.TipoRetirada, f.favorecido'
      
        'From   (((Retiradas r INNER JOIN RetiradasDoc rd ON r.Retirada =' +
        ' rd.Retirada)'
      
        '       LEFT  JOIN Favorecidos f ON f.Favorecido = rd.FavorecidoD' +
        'oc)'
      '       INNER JOIN Contas ct ON ct.Conta = rd.Conta)'
      '       LEFT JOIN StatusDoc sd ON sd.Status = rd.Status'
      
        '       INNER JOIN FormasPagamento fp on rd.Formapagamento = fp.f' +
        'ormapagamento'
      ''
      'Where fp.Especie = 1')
    UpdateObject = U_RetiradasDoc
    Left = 21
    Top = 158
  end
