  object Q_Cheques: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      
        'Select d.Favorecido, dd.IDDoc, dd.*, f.Nome, s.Descricao as Desc' +
        'Status,'
      
        'f.Endereco, f.Cidade, f.Uf, f.Fone1, c.Descricao as NomeConta, b' +
        '.Nome as NomeBanco, d.TipoDeposito'
      
        'From DepositosDoc dd, Depositos d, Favorecidos f, FormasPagament' +
        'o fp, '
      '         StatusDoc s, Contas c, Favorecidos b'
      
        'Where dd.Deposito = d.Deposito and d.Favorecido = f.Favorecido a' +
        'nd d.Conta = c.Conta'
      'and      dd.FormaPagamento = fp.FormaPagamento'
      'and      dd.Banco = b.Favorecido'
      'and      dd.Status = s.Status'
      'and      dd.Status < 70  '
      'and      fp.Especie = 1'
      'and      not (dd.ContaAtual is null)')
    UpdateObject = U_Cheques
    Left = 46
    Top = 203
  end
