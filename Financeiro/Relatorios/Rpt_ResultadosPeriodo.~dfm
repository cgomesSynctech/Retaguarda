  object Q_SaidasItem: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select'
      'sum(si.customedio * si.quantidade) as custo'
      'FROM saidas s'
      'INNER JOIN saidasitens si ON s.saida = si.saida'
      
        'WHERE s.tipomovimento in(1, 7) AND s.data between :datai and :da' +
        'taf'
      ' and s.situacao <> '#39'C'#39)
    Left = 306
    Top = 152
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end
