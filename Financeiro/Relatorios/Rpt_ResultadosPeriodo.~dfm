  object Q_Saida: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'SELECT'
      'SUM(s.valoricms) AS valoricms,'
      'sum(s.frete + s.outrasdespesas) as despesas,'
      'sum(s.desconto*-1)  as desconto,'
      'sum(s.totalitens) as totalpago'
      'FROM saidas s'
      
        'WHERE s.tipomovimento in(1, 7) AND s.data between :datai and :da' +
        'taf'
      ' and s.situacao <> '#39'C'#39' ')
    Left = 304
    Top = 42
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dataf'
        ParamType = ptUnknown
      end>
