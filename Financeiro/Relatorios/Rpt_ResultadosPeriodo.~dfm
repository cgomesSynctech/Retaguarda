  object Q_Entrada: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'select'
      '  sum(e.valoricms) as valoricms'
      'from entradas e'
      'WHERE'
      'e.tipomovimento in (4, 12) and'
      'e.situacao <> '#39'C'#39' AND e.data between :datai and :dataf')
    Left = 358
    Top = 34
    ParamData = <
