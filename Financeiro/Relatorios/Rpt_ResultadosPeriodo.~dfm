  object Q_Detalhe: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    OnCalcFields = Q_DetalheCalcFields
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      ''
      
        'SELECT i.codigo, i.descricao, i.referencia, f.descricao as Fabri' +
        'cante, si.unidade, g.descricaogrupo,'
      'sum(si.quantidade) As Qtd,'
      'Max(si.customedio) as  CustoProduto,'
      'sum(si.customedio * si.quantidade) as Custo,'
      'sum(si.preco * si.quantidade)  as PrcVenda,'
      'Sum(si.valoricmsprod)  AS valoricms,'
      'Max(s.frete) + Max(s.outrasdespesas)   as despesas,'
      'Sum(s.desconto*-1)  as desconto'
      'FROM saidas s'
      '         Left join saidasitens si on si.saida = s.saida'
      '         Left join Itens i on i.item = si.item'
      '         Left Join grupos g on i.grupo = g.grupo'
      '         Left Join fabricantes f on f.fabricante = i.fabricante'
      
        '         Left Join Itensunidades iu on iu.item = i.item and iu.u' +
        'nidade = i.unidade'
      
        'WHERE s.tipomovimento in(1, 7)  and s.data between :datai and :d' +
        'ataf and s.situacao <> '#39'C'#39' '
      
        'Group by i.codigo, i.descricao, i.referencia, f.descricao, si.un' +
        'idade, g.descricaogrupo')
    Left = 200
    Top = 3
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'datai'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'dataf'
        ParamType = ptUnknown
      end>
    object Q_DetalheCODIGO: TIBStringField
      FieldName = 'CODIGO'
      Size = 13
    end
    object Q_DetalheDESCRICAO: TIBStringField
      FieldName = 'DESCRICAO'
      Size = 50
    end
    object Q_DetalheREFERENCIA: TIBStringField
      FieldName = 'REFERENCIA'
      Size = 25
    end
    object Q_DetalheFABRICANTE: TIBStringField
      FieldName = 'FABRICANTE'
      Size = 30
    end
    object Q_DetalheUNIDADE: TIBStringField
      FieldName = 'UNIDADE'
      Size = 6
    end
    object Q_DetalheicPercentual: TFloatField
      FieldKind = fkCalculated
      FieldName = 'icPercentual'
      DisplayFormat = '#00.00'
      Calculated = True
    end
    object Q_DetalheDESCRICAOGRUPO: TIBStringField
      FieldName = 'DESCRICAOGRUPO'
      Size = 30
    end
    object Q_DetalheQTD: TFloatField
      FieldName = 'QTD'
    end
    object Q_DetalheCUSTOPRODUTO: TFloatField
      FieldName = 'CUSTOPRODUTO'
    end
    object Q_DetalheCUSTO: TFloatField
      FieldName = 'CUSTO'
    end
    object Q_DetalhePRCVENDA: TFloatField
      FieldName = 'PRCVENDA'
    end
    object Q_DetalheVALORICMS: TFloatField
      FieldName = 'VALORICMS'
    end
    object Q_DetalheDESPESAS: TFloatField
      FieldName = 'DESPESAS'
    end
    object Q_DetalheDESCONTO: TFloatField
      FieldName = 'DESCONTO'
    end
  end
