  object Q_Retiradas: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select c.Descricao, Sum(t.Valor) as Valor'
      'From Transacoes t '
      
        'inner join ContasOperacao co on (t.idgerador = co.idgerador) and' +
        ' (co.empresa = t.empresa) and (co.pdv = t.pdv)'
      'inner join Contas c on co.conta = c.conta'
      
        'Where t.Data = :Data and t.Conta = :Conta and t.TipoTransacao = ' +
        '''D'' and t.Situacao is null'
      'Group by c.Descricao'
      '')
    Left = 216
    Top = 40
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Data'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'Conta'
        ParamType = ptUnknown
      end>
    object Q_RetiradasDESCRICAO: TIBStringField
      FieldName = 'DESCRICAO'
      Size = 50
    end
    object Q_RetiradasVALOR: TIBBCDField
      FieldName = 'VALOR'
      Precision = 18
      Size = 2
    end
  end
  object Q_Cartoes: TIBQuery
    Database = DMProjeto.DB_Projeto
    Transaction = DMProjeto.IBT_Projeto
    BufferChunks = 1000
    CachedUpdates = False
    SQL.Strings = (
      'Select fp.descricao, Sum(t.Valor) as Total '
      'From Transacoes t '
      
        'Inner join FormasPagamento fp on t.formapagamento = fp.formapaga' +
        'mento '
      'Where t.Data = :Data and '
      '           t.Conta = :Conta and '
      '           fp.especie = 3 and'
      '           t.tipotransacao = ''C'''
      'Group by fp.descricao')
    Left = 296
    Top = 40
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'Data'
        ParamType = ptUnknown
      end
      item
        DataType = ftUnknown
        Name = 'Conta'
        ParamType = ptUnknown
      end>
    object Q_CartoesDESCRICAO: TIBStringField
      FieldName = 'DESCRICAO'
      Required = True
      Size = 50
    end
    object Q_CartoesTOTAL: TIBBCDField
      FieldName = 'TOTAL'
      Precision = 18
      Size = 2
    end
  end
